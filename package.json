{
  "name": "expense-tracker",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "node --require ./setup-proxy.cjs ./node_modules/.bin/observable preview",
    "build": "node --require ./setup-proxy.cjs ./node_modules/.bin/observable build",
    "build:no-proxy": "unset HTTPS_PROXY HTTPS_PROXY HTTP_PROXY http_proxy https_proxy GLOBAL_AGENT_HTTP_PROXY && observable build",
    "deploy": "wrangler pages deploy dist --project-name=expense-tracker"
  },
  "devDependencies": {
    "@observablehq/framework": "^1.12.0",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "wrangler": "^4.50.0"
  },
  "dependencies": {
    "@observablehq/plot": "^0.6.17",
    "d3": "^7.9.0",
    "d3-dsv": "^3.0.1",
    "d3-time-format": "^4.1.0",
    "global-agent": "^3.0.0",
    "htl": "^0.3.1",
    "undici-proxy": "^0.3.0"
  }
}
